<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="1.5"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50sp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginRight="3dp"
                    android:background="@drawable/top_round_square"
                    android:drawableLeft="@drawable/header_img_reduction"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="5dp"
                    android:shadowColor="#042a47"
                    android:tag="rawBarCd"
                    android:text="@string/rawBarCd"
                    android:textColor="#fff"
                    android:textSize="20dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/rawBarCd"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|top"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:layout_marginBottom="3dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="6dp"
                    android:background="@drawable/bottom_round_square"
                    android:bufferType="spannable"
                    android:gravity="center_vertical|left"
                    android:textColor="#040500"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:lineSpacingMultiplier="1.5"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="5"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50sp"
            android:layout_gravity="top"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/standardBarCd"
                android:tag="standardBarCd"
                android:layout_gravity="center_horizontal|center_vertical"
                android:gravity="center_vertical|left"
                android:background="@drawable/top_round_square"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="3dp"
                android:layout_marginTop="3dp"
                android:textColor="#fff"
                android:textStyle="bold"
                android:textSize="20dp"
                android:drawableLeft="@drawable/header_img_reduction"
                android:paddingLeft="5dp"

                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="5"
            android:orientation="horizontal"
            android:layout_gravity="center_horizontal|center_vertical"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            >
            <LinearLayout
                android:layout_width="0sp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:text="구분"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="div"
                        android:textColor="#040500"
                        android:background="@drawable/gray_square"
                        android:textStyle="bold"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/header"
                        android:tag="header"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/gray_square"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="4"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0sp"
                        android:layout_height="match_parent"
                        android:layout_weight="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/specInfo"
                            android:tag="specInfo"
                            android:layout_gravity="center_horizontal|center_vertical"
                            android:gravity="center_vertical|center_horizontal"
                            android:textStyle="bold"
                            android:textColor="#040500"
                            android:background="@drawable/gray_square"/>


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0sp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/compCd"
                                android:tag="compCd"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/partNo"
                                android:tag="partNo"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/orderCd"
                                android:tag="orderCd"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/eoNo"
                                android:tag="eoNo"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="4"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0sp"
                        android:layout_height="match_parent"
                        android:layout_weight="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="@string/trackInfo"
                            android:tag="trackInfo"
                            android:layout_gravity="center_horizontal|center_vertical"
                            android:gravity="center_vertical|center_horizontal"
                            android:textStyle="bold"
                            android:textColor="#040500"
                            android:background="@drawable/gray_square"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0sp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/prodDate"
                                android:tag="prodDate"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/part4m"
                                android:tag="part4m"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/aOr"
                                android:tag="aOr"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0sp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/trackNo"
                                android:tag="trackNo"
                                android:layout_gravity="center_horizontal|center_vertical"
                                android:gravity="center_vertical|center_horizontal"
                                android:textStyle="bold"
                                android:textColor="#040500"
                                android:background="@drawable/gray_square"/>
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/tailer"
                        android:tag="tailer"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/gray_square"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0sp"
                android:layout_height="match_parent"
                android:layout_weight="0.7"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/result"
                        android:tag="result"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/gray_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result0"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result1"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result2"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result3"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result4"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result5"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result6"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result7"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result8"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/result9"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#0405dc"
                        android:textSize="18dp"
                        android:background="@drawable/white_square"/>
                </LinearLayout>


            </LinearLayout>
            <LinearLayout
                android:layout_width="0sp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/data"
                        android:tag="data"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/gray_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data0"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data1"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/data2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data3"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data4"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data5"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data6"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data7"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data8"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0sp"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/data9"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:gravity="center_vertical|center_horizontal"
                        android:textStyle="bold"
                        android:textColor="#040500"
                        android:background="@drawable/white_square"/>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="@drawable/bottom_line">

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="0.9"
        android:orientation="horizontal"
        android:background="@drawable/stripe">
        <Button
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/langs"
            android:tag="langs"
            android:id="@+id/btnLang"
            android:textColor="#fff"
            android:textStyle="bold"
            android:textSize="19dp"
            android:color="#0075ba"
            android:background="@drawable/button_square"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="13dp"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="1.5dp"/>
        <Button
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/history"
            android:tag="history"
            android:id="@+id/btnHistory"
            android:textColor="#fff"
            android:textStyle="bold"
            android:textSize="19dp"
            android:color="#0075ba"
            android:background="@drawable/button_square"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="13dp"
            android:layout_marginLeft="1.5dp"
            android:layout_marginRight="1.5dp"/>
        <Button
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/scan"
            android:tag="scan"
            android:id="@+id/btnScan"
            android:textColor="#fff"
            android:textStyle="bold"
            android:textSize="19dp"
            android:color="#0075ba"
            android:background="@drawable/button_square"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="13dp"
            android:layout_marginLeft="1.5dp"
            android:layout_marginRight="3dp"/>
    </LinearLayout>

</LinearLayout>